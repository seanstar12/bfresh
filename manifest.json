{
  "name": "Dubs Getter Pro (4Chan)",
  "short_name": "DubsGet",
  "manifest_version": 2,
  "version": "1.13",
  "description": "Guaranteed Dubs Everytime, None of the Time",
  "icons": {
    "64": "bfresh64.png",
    "128": "bfresh128.png"
  },
  "content_scripts": [ 
    {
      "js": [
        "js/script.js",
        "js/onStart.js"
      ],
      "matches": [ "https://boards.4chan.org/*/", "http://boards.4chan.org/*/" ],
      "run_at": "document_start"
    },
    {
      "js": [
        "js/onStart.js"
      ],
      "matches": [ "https://boards.4chan.org/*/", "http://boards.4chan.org/*/" ],
      "run_at": "document_start"
    }, 
    {
      "js": [
        "js/functions.js",
        "js/onEnd.js"
      ],
      "matches": [ "https://boards.4chan.org/*/", "http://boards.4chan.org/*/" ],
      "run_at": "document_end"
    } 
  ],
  "web_accessible_resources": [
    "js/*"
  ]
}
