{
  "name": "Dubs Getter Pro (4Chan)",
  "short_name": "DubsGet",
  "manifest_version": 2,
  "version": "1.15",
  "description": "Guaranteed Dubs Everytime, None of the Time",
  "icons": {
    "64": "bfresh64.png",
    "128": "bfresh128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [ 
    {
      "js": [ "js/script.js" ],
      "matches": [ "*://boards.4chan.org/*/" ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "js/script"
  ]
}
